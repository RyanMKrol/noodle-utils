{
  "name": "noodle-utils",
  "version": "1.0.1",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/RyanMKrol/noodle-utils.git"
  },
  "license": "MIT",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "npm run lint && shx rm -rf build && ./node_modules/.bin/babel src --out-dir build",
    "docs": "doxdox 'src/**/*.js' --layout markdown --output README.md",
    "lint": "eslint src/ --ext .js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/husky"
    }
  },
  "dependencies": {
    "noodle-dynamo": "^1.0.6",
    "sizeof": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "doxdox": "^3.0.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.4.1",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.5",
    "shx": "^0.3.2"
  }
}
